<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"> p := RandPrime(5);
myRand := rand(1..p):
g := myRand() mod p;
a := myRand();
A := ((g^a) mod p);
message := 69;
k := myRand() mod p;
S1 := ((g^k) mod p);
(message - (a * S1)) * (k^(-1));
S2 := ((message - (a * S1))*(k^(-1)) mod (p-1));
if ((A^(S1) * S1^S2) = ((g^message) mod p)) then
  return true;
end if;



