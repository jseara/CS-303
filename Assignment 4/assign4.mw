<Input><Text-field prompt="&gt; " style="Maple Input" spaceabove="0" linebreak="space" rightmargin="0" linespacing="0.0" initial="0" firstindent="0" leftmargin="0" bulletsuffix="" alignment="left" bullet="none" spacebelow="0" pagebreak-before="false">    while (y = 1) do   
       b := nextprime(b);  c := c + 1;  print(i,b,F[i]);
       if (igcd(b,n) &lt;&gt; 1) then
          return false;
       end if;
